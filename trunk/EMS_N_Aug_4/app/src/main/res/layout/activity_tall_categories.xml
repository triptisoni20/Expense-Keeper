<ScrollView
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_height="wrap_content"
android:layout_width="fill_parent" >

<TableLayout
    android:id="@+id/tablelayout"
    android:layout_height="wrap_content"
    android:layout_width="fill_parent"
    android:paddingRight="2dip"
    android:stretchColumns="*"
    >

    <TableRow>
        <Button
            style="@style/ButtonStandard"
            android:layout_column="0"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:text="Food"
            android:tag="@integer/int1"
            android:id="@+id/button1"
            />
        <Button
            style="@style/ButtonStandard"
            android:layout_column="1"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:tag="@integer/int2"
            android:id="@+id/button2"
            android:text="Entertainment"

            />
        <Button
            style="@style/ButtonStandard"
            android:layout_column="2"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:tag="@integer/int3"
            android:id="@+id/button3"
            android:text="Automobile" />
    </TableRow>
    <TableRow>
        <Button
            style="@style/ButtonStandard"
            android:layout_column="0"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:tag="@integer/int4"
            android:id="@+id/button4"

            android:text="Transportation" />
        <Button
            style="@style/ButtonStandard"
            android:layout_column="1"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:id="@+id/button5"

            android:tag="@integer/int5"

            android:text="Groceries" />
        <Button
            style="@style/ButtonStandard"
            android:layout_column="2"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:tag="@integer/int6"
            android:id="@+id/button6"

            android:text="Bills" />
    </TableRow>
    <TableRow>
        <Button
            style="@style/ButtonStandard"
            android:layout_column="0"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:tag="@integer/int7"
            android:id="@+id/button7"

            android:text="Rent" />
        <Button
            style="@style/ButtonStandard"
            android:layout_column="1"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:tag="@integer/int8"
            android:id="@+id/button8"

            android:text="Medical" />
        <Button
            style="@style/ButtonStandard"
            android:layout_column="2"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:tag="@integer/int9"
            android:id="@+id/button9"
            android:text="Shopping" />
    </TableRow>
    <TableRow>

        <Button
            style="@style/ButtonStandard"
            android:layout_column="0"
            android:layout_marginTop="6dip"
            android:onClick="onClickOk"
            android:tag="@integer/int10"
            android:id="@+id/button10"
            android:text="Misc"
            android:layout_span="3" />
    </TableRow>


</TableLayout>
</ScrollView>
